<!DOCTYPE html>
<html>
  <head>
    <title>GroupAssignment</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>


    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div id="main">
      <div id="header">
      <h1>Professor and Offices</h1>
      <%= link_to  "Professors", professors_path %>
      <%= link_to  "Offices", offices_path %>
      <%= link_to  "Assignments", assignments_path %>
      <%= link_to  "Courses", courses_path %>
      </div>
    </div>
    <%= yield %>
  </body>
</html>
